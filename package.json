{
  "name": "draftkings-chrome-extension",
  "version": "0.0.0-development",
  "description": "DraftKings Chrome Extension",
  "main": "index.js",
  "scripts": {
    "build": "rm -rf build && webpack --mode=development",
    "build:prod": "rm -rf build && NODE_ENV=production webpack --mode=production",
    "codecov": "codecov",
    "commitmsg": "commitlint -e $GIT_PARAMS",
    "lint": "eslint --ext .js .",
    "semantic-release": "semantic-release",
    "test": "jest --coverage --passWithNoTests",
    "travis-deploy-once": "travis-deploy-once",
    "watch": "webpack --mode=development --watch"
  },
  "keywords": [
    "draftkings",
    "chrome"
  ],
  "author": "jae.b.bradley@gmail.com",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/jaebradley/draftkings-chrome-extension.git"
  },
  "devDependencies": {
    "@babel/cli": "^7.1.5",
    "@babel/core": "^7.1.6",
    "@babel/plugin-transform-async-to-generator": "^7.1.0",
    "@babel/plugin-transform-runtime": "^7.1.0",
    "@babel/preset-env": "^7.1.6",
    "@babel/runtime": "^7.1.5",
    "@commitlint/cli": "^7.2.1",
    "@commitlint/config-angular": "^7.1.2",
    "@commitlint/prompt": "^7.2.1",
    "@commitlint/prompt-cli": "^7.2.1",
    "babel-core": "^7.0.0-bridge.0",
    "babel-jest": "^23.6.0",
    "babel-loader": "^8.0.4",
    "chrome-webstore-upload-cli": "^1.2.0",
    "codecov": "^3.1.0",
    "copy-webpack-plugin": "^4.6.0",
    "eslint": "^5.9.0",
    "eslint-config-airbnb-base": "^13.1.0",
    "eslint-plugin-import": "^2.14.0",
    "fs-extra": "^7.0.1",
    "husky": "^1.2.0",
    "jest": "^23.6.0",
    "semantic-release": "^15.12.1",
    "travis-deploy-once": "^5.0.9",
    "uglifyjs-webpack-plugin": "^2.0.1",
    "webpack": "^4.26.0",
    "webpack-cli": "^3.1.2"
  },
  "dependencies": {
    "axios": "^0.18.0",
    "cheerio": "^1.0.0-rc.2",
    "element-ready": "^3.0.0"
  }
}
